{
  "name": "unstorage",
  "version": "1.12.0",
  "description": "Universal Storage Layer",
  "repository": "unjs/unstorage",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./drivers/*": {
      "types": "./drivers/*.d.ts",
      "import": "./drivers/*.mjs",
      "require": "./drivers/*.cjs"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "drivers",
    "server.d.ts"
  ],
  "scripts": {
    "bench": "jiti test/server.bench.ts",
    "build": "unbuild",
    "demo": "vite demo",
    "dev": "vitest",
    "lint": "eslint . && prettier -c src test demo",
    "lint:fix": "eslint . --fix && prettier -w src test demo",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck",
    "unstorage": "pnpm jiti src/cli"
  },
  "dependencies": {
    "anymatch": "^3.1.3",
    "chokidar": "^3.6.0",
    "destr": "^2.0.3",
    "h3": "^1.13.0",
