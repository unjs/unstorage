{
  "name": "unstorage",
  "version": "1.0.1",
  "description": "Universal Storage Layer",
  "repository": "unjs/unstorage",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./drivers/*": {
      "import": "./dist/drivers/*.mjs",
      "require": "./dist/drivers/*.cjs",
      "types": "./dist/drivers/*.d.ts"
    },
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "drivers"
  ],
  "scripts": {
    "build": "unbuild",
    "demo": "vite demo",
    "dev": "vitest",
    "doctoc": "doctoc README.md --notitle --maxlevel 3 && prettier -w README.md",
    "lint": "eslint --ext .ts . && prettier -c src test demo",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && git push --follow-tags && pnpm publish",
    "test": "pnpm lint && vitest run --coverage",
    "unstorage": "pnpm jiti src/cli"
  },
  "dependencies": {
    "anymatch": "^3.1.3",
    "chokidar": "^3.5.3",
    "destr": "^1.2.2",
    "h3": "^1.1.0",
    "ioredis": "^5.3.0",
    "listhen": "^1.0.2",
    "mkdir": "^0.0.2",
    "mri": "^1.2.0",
    "ofetch": "^1.0.0",
    "ufo": "^1.0.1",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230115.0",
    "@types/jsdom": "^21.1.0",
    "@types/mri": "^1.1.1",
    "@types/node": "^18.11.19",
    "@types/ws": "^8.5.4",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.4",
    "@vue/compiler-sfc": "^3.2.47",
    "c8": "^7.12.0",
    "changelogen": "^0.4.1",
    "doctoc": "^2.2.1",
    "eslint": "^8.33.0",
    "eslint-config-unjs": "^0.1.0",
    "jiti": "^1.16.2",
    "jsdom": "^21.1.0",
    "monaco-editor": "^0.34.1",
    "msw": "^1.0.0",
    "prettier": "^2.8.3",
    "types-cloudflare-worker": "^1.2.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.1",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vue": "^3.2.47"
  },
  "packageManager": "pnpm@7.26.3"
}
